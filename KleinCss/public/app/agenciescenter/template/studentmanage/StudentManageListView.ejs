<div class="ibox-content">
    <div class="tab-content">
        <div id="tab-1" class="tab-pane active">
            <div id="agencies-studentmanage-listscroll" class="full-height-scroll">
                <div class="table-responsive">
                    <table class="table table-striped table-hover">
                        <thead>
                        <tr>
                            <th><h5><%= locale.StudentManage.No %></h5></th>
                            <th><h5><%= locale.StudentManage.StudentName %></h5></th>
                            <th></th>
                        </tr>
                        </thead>
                        <tbody>
                        <%for(var i=0;i<students.reID.length;i++){ %>
                        <tr>
                            <td><%= i+1%></td>
                            <td><a data-toggle="tab"  class="client-link"><%= students.reName[i]%></a></td>
                            <td><a id="agencies-studentmanage-allfind" num = "<%= i%>" class="agencies-studentmanage-allfind btn btn-xs btn-outline btn-primary"><%= locale.StudentManage.More %> <i class="fa fa-long-arrow-right"></i> </a></td>

                        </tr>
                        <% }%>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>


