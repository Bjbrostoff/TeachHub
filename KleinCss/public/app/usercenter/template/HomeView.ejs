                            <div class="row" id="home-manage-act-today">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-4 col-md-4">
            <div class="ibox">
                <div class="ibox">
                    <div class="row" id="home-manage-act-right">
                    </div>
                    <div class="row">
                        <div class="row">
                            <div class="row">
                                <div class="col-md-8 col-md-offset-1 col-sm-8 col-sm-offset-1">
                                    <strong><p><%=locale.Home.courseArranged%>:</p></strong>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div id="home-arrange-calendar" class="col-md-9 col-md-offset-1 col-sm-9 col-sm-offset-1">
