<% var count = datas.length; %>
<% for (var i = 0;i < count;i++){ %>
<% if(i % 2 == 0){ %>
<div class="panel-group">
    <div class="row">
        <div class="course-manager-chartLegend-item col-md-4 col-md-offset-2" >
            <span class="label" style="background-color: <%=colors[i]%>"> </span>
                    <span>&nbsp;
                        <%= datas[i]['label'] %>
                    </span>
            <strong><%= datas[i].value %></strong>
        </div>
        <% }else{ %>
        <div class="course-manager-chartLegend-item col-md-6" >
            <span class="label" style="background-color: <%=colors[i]%>"> </span>
            <span>&nbsp;<%= datas[i]['label'] %></span>
            <strong><%= datas[i]['value'] %></strong>
        </div>
    </div>
</div>
<% } %>
<% } %>