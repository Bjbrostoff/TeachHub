<% var count = datas.length; %>
<% for (var i = 0;i < count;i++){ %>
<% if(i % 2 == 0){ %>
<div class="panel-group">
    <div class="row">
        <div class="fav-chart-item col-md-4 col-md-offset-2" style="cursor: hand" data-type="<%= datas[i]['type'] %>">
            <span class="label" style="background-color: <%=colors[i]%>"> </span>
                    <span>&nbsp;
                        <%= datas[i]['label'] %>
                    </span>
            <strong><%= datas[i].value %></strong>
        </div>
        <% }else{ %>
        <div class="fav-chart-item col-md-6" style="cursor: hand" data-type="<%= datas[i]['type'] %>">
            <span class="label" style="background-color: <%=colors[i]%>"> </span>
            <span>&nbsp;<%= datas[i]['label'] %></span>
            <strong><%= datas[i]['value'] %></strong>
        </div>
    </div>
</div>
<% } %>
<% } %>

