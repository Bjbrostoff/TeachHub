                    <%=fields.hwcontent%>
                </div>
            </div>
        </div>
    </div>
    <div class="row col-sm-12" style="position: absolute;top: 567px;">
        <div class="col-sm-3">
        </div>
        <div class="col-sm-3">
            <div id="myhomework-editview-save" class="btn btn-primary "><%= locale.MyHomework.Save %></div>
