        <div class="col-sm-3">
            <div id="myhomework-editview-submit" class="btn btn-primary "><%= locale.MyHomework.Submit %></div>
        </div>
        <div class="col-sm-3">
            <div class=" myhomework-editview-close btn btn-default"><%= locale.MyHomework.Close %></div>
        </div>

    </div>
</div>




