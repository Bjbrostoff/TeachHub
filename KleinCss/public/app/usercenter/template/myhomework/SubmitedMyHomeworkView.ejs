<div class="sameday-box" style="display:block;"><!--当日详情-->
    <table class="table-box sameday-table">
        <tr>
            <td><%= locale.MyHomework.No %></td>
            <td><%= locale.MyHomework.Homeworktitle %></td>
            <td><%= locale.MyHomework.Timeofsubmission %></td>
            <td></td>
        </tr>
        <% var i = 0; %>
        <% _.each(datas, function(item){ %>
        <tr>
            <td><%= ++i %></td>
            <td class="project-title">
                <%= item.hwid.hwtitle %>
            </td>
            <td>
                <%= (item.commitdate.slice(0, 10) + ' ' + item.commitdate.slice(11, 16)) %>

            </td>
            <td class="project-actions">
                <button modelid="<%= item.id %>" class="mhw-find-view-tab btn btn-xs btn-info" style="background-color: #337ab7;color: #fff" type="button">
                    <%= locale.MyHomework.View %>
                </button>
            </td>
        </tr>
        <% }) %>

    </table>

</div>