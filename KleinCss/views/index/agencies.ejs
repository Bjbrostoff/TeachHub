<% include ../includes/footer/normal.ejs %>


</body>
<script>
    var user_login = false;
    <% if (login){ %>
    user_login = true;
    <% } %>
    var localeName = '<%= localeName %>';
    $('.top-dropdown-btn').dropdown()
    $('.top-dropdown-chosebtn').click(function(){
                var all = $('#top-dropdown-menu').find('.top-dropdown-chosebtn');
                for (var i = 0; i < all.length; i++){
                    $(all[i]).removeClass('top-dropdown-chosebtn-active');
                }
                $(this).addClass('top-dropdown-chosebtn-active');
                $('.top-dropdown-btn').html( $(this).html()+'&nbsp;<span class="caret"></span>');
            }
    )

    $('.top-search-btn').click(function(){
        var type = $('.top-dropdown-chosebtn-active').attr('topsearchtag');
        var text = $('#web_search_header').val();
        //console.log(text);
        window.location.href = '/'+type+'#search/'+text
    })
</script>
<script src="/requirejs/require.js" data-main="/app/agencies/agenciesmain.js"></script>
</html>