<div id="agencies-condition-view" class="container page-wrapper-search">

</div>

<div id="agencies-result-view" class="container page-wrapper-result">

</div>

<div>
    <div class="main-body container clearfix">
        <div id="agencies-profile-detail" class="l profile-detail">
        </div>
        <div id="agencies-profile" class="r space-main profile-course">
        </div>
        <div id="map-container" class="row">
            <div id="map" class="col-lg-12" style="height: 350px;">

            </div>
        </div>
